{
  "name": "markademic",
  "version": "0.4.0",
  "description": "üè´ A markdown parser with academic features included.",
  "main": "dist/markademic.js",
  "typings": "./markademic.d.ts",
  "scripts": {
    "build:develop": "cross-env NODE_ENV=development webpack src/markademic.ts dist/markademic.js",
    "build:production": "cross-env NODE_ENV=production webpack src/markademic.ts dist/markademic.js",
    "build:publish": "npm run build:develop && npm run build:production",
    "pretest": "tsc",
    "test": "ava tests/markademic.spec.js",
    "coverage": "nyc npm test | nyc report --reporter=lcov | codecov",
    "test:watch": "ava --converage --watch",
    "dts": "dts-generator --project ./ --exclude ./**/*.d.ts --main ./src/markademic.ts --name markademic --out dist/markademic.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperfuse/markademic.git"
  },
  "keywords": [
    "markdown",
    "academic",
    "paper",
    "publication",
    "latex",
    "syntax highlighting",
    "citation",
    "bibjson"
  ],
  "author": "Alain Galvan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyperfuse/markademic/issues"
  },
  "homepage": "https://github.com/hyperfuse/markademic#readme",
  "devDependencies": {
    "@types/katex": "0.0.29",
    "@types/node": "^6.0.52",
    "ava": "^0.17.0",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "nyc": "^10.0.0",
    "ts-loader": "^1.3.3",
    "ts-node": "^1.7.2",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.1"
  },
  "dependencies": {
    "highlight.js": "^9.9.0",
    "remarkable": "^1.7.1",
    "katex": "^0.6.0"
  }
}
