{
  "name": "markademic",
  "version": "0.2.0",
  "description": "üè´ A markdown parser with academic features included.",
  "main": "dist/markademic.js",
  "typings": "./dist/markademic.d.ts",
  "scripts": {
    "build:develop": "cross-env NODE_ENV=development webpack src/markademic.ts dist/markademic.js",
    "build:production": "cross-env NODE_ENV=production webpack src/markademic.ts dist/markademic.min.js",
    "build:publish": "npm run build:develop && npm run build:production",
    "test": "ava ./tests",
    "coverage": "ava && codecov",
    "test:watch": "ava --converage --watch",
    "dts": "dts-generator --project ./ --exclude ./**/*.d.ts --main ./src/markademic.ts --name markademic --out dist/markademic.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stelatech/markademic.git"
  },
  "keywords": [
    "markdown",
    "academic",
    "paper",
    "publication",
    "latex",
    "syntax highlighting",
    "citation",
    "bibjson"
  ],
  "author": "Alain Galvan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stelatech/markademic/issues"
  },
  "homepage": "https://github.com/stelatech/markademic#readme",
  "devDependencies": {
    "@types/katex": "0.0.29",
    "@types/node": "^6.0.45",
    "ava": "^0.16.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.1.2",
    "dts-generator": "^2.0.0",
    "ts-loader": "^0.9.1",
    "typescript": "^2.0.3",
    "webpack": "^2.1.0-beta.22"
  },
  "peerDependencies": {
    "highlight.js": "^9.6.0",
    "remarkable": "^1.6.2",
    "katex": "^0.6.0"
  },
  "dependencies": {}
}